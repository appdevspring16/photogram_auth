<div class="page-header">
  <h1>
    Users

  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
      </tr>

      <% @users.each do |user| %>
      <tr>
          <li><a href="/userinfo/<%= user.id %>"><%= user.username %></a></li>
      </tr>
      <% end %>
    </table>
  </div>
</div>
