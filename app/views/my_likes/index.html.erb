
<div class="col-md-8 col-md-offset-2">

  <% @user.liked_photos.each do |my_like| %>
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">
          <a href="/photos/<%=my_like.id%>"><%=@user.username%></a> <small><%=time_ago_in_words(Time.now) %></small>
        </h3>
      </div>

      <div class="panel-body">
        <a href="/photos/<%=my_like.id%>">
          <img src="<%= my_like.image %>" class="img-responsive">
        </a>
      </div>
      </div>

        <% end %>
</div>

