<div class="page-header">
  <h1>
    My Likes
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">

      <% current_user.liked_photos.each do |photo| %>
      <tr>
        <td><img src="<%= photo.image %>"></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
