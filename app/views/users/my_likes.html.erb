<div class="page-header">
  <h1>
    My Likes
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
        <th>Photo</th>
      </tr>

      <% @my_likes.each do |my_like| %>
      <tr>
        <td><%= my_like.user_id %></td>
        <td><%= my_like.photo_id %></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
