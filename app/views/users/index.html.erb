<div class="container">
<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <table class="table table-hover table-bordered panel-heading">
      <tr>
        <th>All Users</th>
      </tr>

      <% @users.each do |user| %>
      <tr>
        <td>  <a href = "/users/<%=user.id %>"><%= user.username %></a></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
</div>
