<div class="page-header">
  <h1>
    Users

    <!-- <a href="/photos/new" class="btn btn-lg btn-success">New Photo</a> -->

  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>

        <!-- <th>Image</th> -->
        <th>Username</th>
        <th>Email</th>


      </tr>

      <% @users.each do |user| %>
      <tr>

        <td><%= user.username %></td>

        <td><%= user.email %></td>

        <td>

          <a href="/users/<%= user.id %>" class="btn btn-primary">Show</a>
          <a href="/users/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_user/<%= user.id %>" class="btn btn-danger" rel="nofollow">Delete</a>

        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
