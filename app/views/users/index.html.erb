<div class="page-header">
  <h1>
    Users
    </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Username</th>
        <th>User ID
        </th>
        <th>Email</th>
      </tr>

      <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.id %></td>
        <td>
          <%= user.email %>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
