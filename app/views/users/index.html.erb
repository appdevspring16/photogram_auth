<div class="page-header">
  <h5>
    All Users
  </h5>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>

        <th>Username</th>

      </tr>

      <% @users.each do |user| %>
      <tr>

        <td><a href="/users/<%= user.id %>"><%= user.username %></a></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
