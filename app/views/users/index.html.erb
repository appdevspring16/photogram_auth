<body>
<div class="container">

<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">
    All Users
  </h3>
      </div>
      <div class="panel-body">
        <% @users.each do |user| %>
        <tr>
                  <div class="">
                      <a href="/users/<%= user.id %>"><%= user.username.capitalize %> </a>
                  </div>
                  <hr>




        </tr>
        <% end %>

      </div>
  </div>
</div>


</div>
</div>
</body>
