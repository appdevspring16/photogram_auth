<div class="page-header">
  <h1>
    All Users
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>All Users</th>
      </tr>

      <tr>
        <% @users.each do |user| %>
        <td class="list-group-item"><%= user.id %></td>
        <td class="list-group-item"><a href="/users/<%= user.id %>"><%= user.username %></a></li>
      </tr>
      <% end %>
    </table>
  </div>
</div>
