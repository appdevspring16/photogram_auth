<div class="page-header">
  <h2>
    All Users
  </h2>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User ID</th>
        <th>User Email</th>
        <th>Username</th>
      </tr>

        <% @users.each do |user| %>
        <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
        <td><%= user.username %></td>
      </tr>
      <% end %>

    </table>
  </div>
</div>
