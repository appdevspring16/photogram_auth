<div class="col-md-8 col-md-offset-2 page-header jumbotron">
  <h2>
    <%= @user.username %>
  </h1>
</div>

<div class="row">
  <div class="col-md-8 col-md-offset-2">
      <div class="panel">
        <div class="panel-heading">
            <h3><%= @user.username %></h3>
          </div>

          <% @user.photos.each do |photo| %>
            <div class="panel-body">
              <a href="<%= photo.image %>"><img src="<%= photo.image %>" class="img-responsive"/></a>

            </div>
          <% end %>

    </div>
  </div>
</div>
