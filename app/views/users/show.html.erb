<div class="page-header">
  <h3><%= @user.username %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User Photos</th>
      </tr>

      <% @photos.each do |photo| %>
      <tr>
        <td>
          <img src="<%= photo.image %>" alt="" class="img-responsive" />
          <h4><%= photo.caption %></h4>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
