<div class="page-header">
  <h3>My Likes</h3>
</div>

<div class="row">

  <% @myLike.each do |like| %>

  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt><h4><%= User.find_by( :id => Photo.find_by( :id => Like.find_by( :id => like.photo_id )).user_id ).username %></h4></dt>
      <dd><h5><%= Photo.find_by( :id => Like.find_by( :id => like.photo_id )).caption %></h5></dd>

      <dt></dt>
      <dd><img src="<%= Photo.find_by( :id => Like.find_by( :id => like.photo_id )).image %>" height="300" alt="Photo not found"></img></dd>
    </dl>

  </div>

  <% end %>


</div>
